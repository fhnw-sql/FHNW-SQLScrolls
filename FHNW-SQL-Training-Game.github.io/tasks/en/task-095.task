PARSONS {
    SELECT id, type, name, cage_id
    FROM zoo_animal
    WHERE cage_id IS NULL
    SELECT * #distractor
    FROM zoo_animals #distractor
    WHERE cage_id IS false  #distractor
}

ANSWER {
    SELECT id, type, name, cage_id FROM zoo_animal WHERE cage_id IS NULL;
}

