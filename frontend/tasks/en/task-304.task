    name: Car makers
    color: col-book-cars
    info: list car makers by model and car count
    keywords: SELECT, FROM, GROUP BY, HAVING, INTERSECT, JOIN
    level: 3
}

DESCRIPTION {
    How many car makers come from asia? List their Id and FullName.
}

ANSWER {
    SELECT car_makers.Id, car_makers.FullName FROM car_makers JOIN countries ON car_makers.Country = countries.CountryId WHERE countries.Continent = 2;
}

TEST {
