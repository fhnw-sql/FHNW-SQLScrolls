    }

    TABLE {
        continents
        ContId|Continent
        1|europe
        2|asia
    }

    TABLE {
        car_makers
        Id|Maker|FullName|Country
        1|Volkswagen|Volkswagen Group|1
        2|BMW|Bayerische Motoren Werke AG|1
        3|Mercedes|Mercedes-Benz AG|1
