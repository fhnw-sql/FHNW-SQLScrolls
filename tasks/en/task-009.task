DESCRIPTION {
    Show all ghosts ordered from the oldest to the youngest and where they are of the same age, then in alphabetical order.
}

ANSWER {
   SELECT * FROM Ghosts ORDER BY YEAR ASC, NAME ASC
}

STRICT 

TEST {
    TABLE {
        Ghosts
