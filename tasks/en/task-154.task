DESCRIPTION {
    Enola Holmes: "Is there a subject with more people than Data Science?"

    You: "I will list the name and number of people."
}

ANSWER {
    SELECT subject, count(*) amount
    FROM person
    GROUP BY subject
    HAVING count(*)>
    (SELECT count(*) amount
    FROM person
    WHERE subject='Data Science');
}


