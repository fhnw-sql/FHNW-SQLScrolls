DESCRIPTION {
    Which of the ghost names comes first in alphabetical order?
}

ANSWER {
   SELECT MIN(NAME) FROM GHOSTS ORDER BY NAME ASC
}

TEST {
    TABLE {
        Ghosts
        id|name|year
        1|Arthur|1765
        2|Desiree|1801
        3|Siegfried|1650
        4|Sieglinde|1777
        5|Kaaleppi|1833
    }

    RESULT {
        Arthur
    }
}

