DESCRIPTION {
   List all areas and the number of fully skilled magicians in each area, i.e. with skill level 5.
}

ANSWER {
   SELECT area, COUNT(name) FROM wizards WHERE SKILL = 5 GROUP BY area
}


TEST {
    TABLE {
        Wizards
        id|name|area|skill
        1|Anna|Shadowland|5
        2|Orz Orz|Kumpula|1
        3|Bingo|Shadowland|2
        4|Solmu|Shadowland|5
        5|Rufus|Kumpula|1
    }

    RESULT {
        Shadowland|2
    }
}

