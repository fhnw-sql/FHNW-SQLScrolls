DESCRIPTION {
    List all names of ghosts born after 1800.
}

ANSWER {
   SELECT name from ghosts where year > 1800
}

TEST {
    TABLE {
        Ghosts
        id|name|year
        1|Arthur|1765
        2|Desiree|1801
        3|Siegfried|1650
        4|Sieglinde|1777
        5|Kaaleppi|1833
    }

    RESULT {
        Desiree
        Kaaleppi
    }
}

